// Print if a number is odd or even 
import.java.util.*;
public class javaBasics{
  public static void main(String args[]){
    Scanner sc= new scanner(System.in);
    int number = sc.nextInt();
    if(number%2 == 0){
        System.out.println("EVEN");
      }else {
        System.out.println("ODD");
      }
  }
}
